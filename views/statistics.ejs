      <script>
        // for media screen
        $(window).resize(function () {
          if ($(window).width() < 770) {
            console.log("mobile");
            $('#branch-wise').css('width', '450');
            $('#branch-wise').css('height', 'auto');
            $('#degree-wise').css('width', '450');
            $('#title').css('paddingTop', '35%')
            $('#box1').css('width', '25%')
            $('#box1').css('paddingBottom', '5%')
            $('#box2').css('width', '25%')
            $('#box2').css('paddingBottom', '5%')
            $('#box3').css('width', '25%')
            
            // $('#x').css('overflow', 'visible')
            // $('#box3').css('paddingBottom','5%')
            // add class to $('.d-flex')
            $('.d-flex').addClass('align-items-center');
          } else {
            console.log("desktop");
            $('#branch-wise').css('width', '500px');
            $('#degree-wise').css('width', 'auto');
            $('#title').css('paddingTop', '13%');
            $('.x').css('paddingRight', '25%');
            // remove class to $('.d-flex')
            $('.d-flex').removeClass('align-items-center');
          }
        }).resize();
      </script>
      <!-- Js snippet for animation of progress-bars stats -->
      <script>
        window.onload = () => {
          const circles = document.querySelectorAll(".outer");
          const animationDuration = 1500; // Animation duration in milliseconds
          const animationSteps = 100; // Number of steps to complete the animation

          circles.forEach((outerElement, index) => {
            const innerTextElement = outerElement.querySelector(".inner-text");
            const targetPercentages = [61.13 ,65.5, 97]; // Target percentages
            const stepDuration = animationDuration / animationSteps;

            for (let i = 0; i <= animationSteps; i++) {
              setTimeout(() => {
                const percentage = (i * targetPercentages[index]) / animationSteps; // Calculate the actual percentage
                innerTextElement.textContent = `${percentage.toFixed(1)}%`;
                const gradientStops = [
                  `#00ffe0 ${percentage - 3}%`,
                  `rgba(5, 0, 255, 0.8) ${percentage}%`,
                  `white ${percentage}%`,
                  `white ${percentage}%`,
                  `white ${percentage + 27}%`,
                  `white 100%`
                ].join(", ");
                outerElement.style.background = `conic-gradient(${gradientStops})`;

                if (percentage >= targetPercentages[index]) {
                  outerElement.style.animation = "none";
                }
              }, i * stepDuration);
            }
          });
        };
      </script>


